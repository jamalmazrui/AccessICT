<script>
  $('#related-books-rows').on('click', '.book-row a', function(e) {
      var rowTitle = $(this).closest('.book-row').data('title');
      var position = ($(this).closest('.book-tile').index()).toString();
      var element  = $(this).data('alias');
      var title    = $(this).data('title');
      dlPush(rowTitle, position, element, title);
    });

    function dlPush(rowTitle, position, element, title, event){
      window.dataLayer = window.dataLayer || [];dataLayer.push({ 
        'rowTitle'  : rowTitle,
        'position'  : position,
        'element'   : element,
        'title'     : title,
        'rowpage'   : 'catalog',
        'event'     : 'rowSelect',
      });
    }
    function dlEvent(element, title, event){
      window.dataLayer = window.dataLayer || [];dataLayer.push({ 
        'element'   : element,
        'title'     : title,
        'rowpage'   : 'catalog',
        'event'     : 'cover',
      });
    }

    $('.merchpurch select').on('change', function() {
      $(this).siblings('button').prop('disabled', $(this).val() == '');
    });

    $('#related-books-rows').load('rows', function() {
      $('.truncate').dotdotdot({
        watch: true,
        lastCharacter: {
          remove: [' ', ',', ';', '.', '!', '?']
        }
      });
    });

    if ( $('.book-description').children().length > 1 ) {
      $('.book-description').children(':gt(0)').hide();
      $('.book-description').children(':first').after('<a href="#" class="readmore">[read full description]</a>');
      $('.book-description a.readmore').on('click', function(e) {
        e.preventDefault();
        $('.book-description').children(':gt(0)').fadeIn();
